<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lhiot.oc.basic.mapper.OrderStoreMapper">
    <insert id="insert" parameterType="com.lhiot.oc.basic.model.OrderStore" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO order_store (hd_order_code,order_id,store_id,store_name,store_code,operation_user,create_at)
        VALUE (#{hdOrderCode},#{orderId},#{storeId},#{storeName},#{storeCode},#{operationUser},#{createAt})
    </insert>
</mapper>